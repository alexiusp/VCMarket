{
  "name": "vcmarket",
  "version": "1.0.0",
  "description": "VirCities stock and gold market parser",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start_backend": "node app.js",
    "start": "npm run start_backend",
    "start_app": "start http://localhost:5000/app/",
    "start_admin": "start http://localhost:5000/adminApp/",
    "start_all": "npm run start_backend && npm run start_admin && npm run start_app",
    "dev_start": "npm run dev_build && npm run start_all",
    "dev_build": "npm run dev_compile",
    "predev_build":"npm run clean",
    "postdev_build": "npm run cpx:w",
    "cpx":"cpx \"src/**/*.{html,js,png,jpg}\" www",
    "cpx:w":"cpx \"src/**/*.{html,js,png,jpg}\" www -w",
    "clean": "rimraf www/*",
    "dev_compile": "npm run tsc:w",
    "compile": "npm run tsc",
    "build": "npm run compile",
    "prebuild": "npm run clean",
    "postbuild": "npm run cpx",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "typings install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexiusp/VCMarket/"
  },
  "author": "Aleksei Podgaev",
  "license": "MIT",
  "engines": {
    "node": "4.3.1"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "font-awesome": "^4.5.0",
    "qs": "^6.1.0",

    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/compiler-cli": "0.6.0",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/router": "3.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.6",

    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.11",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.17",

    "angular2-in-memory-web-api": "0.0.18",
    "bootstrap": "^3.3.6"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "rimraf": "^2.5.4",
    "typescript": "^2.0.2",
    "typings": "^1.3.2"
  }
}
