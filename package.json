{
  "name": "vcmarket",
  "version": "1.0.0",
  "description": "VirCities stock and gold market parser",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev_start": "concurrently \"npm run dev_build\" \"run.bat\"",
    "predev_start": "npm run clean",
    "cpx": "cpx \"src/**/*.{html,css,png,jpg}\" www",
    "cpx:w": "cpx \"src/**/*.{html,css,png,jpg}\" www -w",
    "clean": "rimraf www/app/* && rimraf www/adminApp/*",
    "compile": "npm run tsc",
    "dev_build": "concurrently --kill-others \"npm run tsc:w\" \"npm run cpx:w\" \"npm run start\"",
    "build": "npm run compile",
    "postbuild": "npm run cpx",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "typings install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexiusp/VCMarket/"
  },
  "author": "Aleksei Podgaev",
  "license": "MIT",
  "engines": {
    "node": "4.3.1"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/compiler-cli": "0.6.0",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/router": "3.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.6",
    "angular2-in-memory-web-api": "0.0.18",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.6",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "font-awesome": "^4.5.0",
    "mongoose": "^4.6.0",
    "qs": "^6.1.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.11",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.17"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "cpx": "^1.5.0",
    "rimraf": "^2.5.4",
    "typescript": "^2.0.2",
    "typings": "^1.3.2"
  }
}
